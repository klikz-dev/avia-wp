{% from 'macros/featured-posts.twig' import featuredPosts %}
{% from 'macros/bullets.twig' import bullets %}

{% for component in post.meta('components') %}
  {% if component.acf_fc_layout == 'form' %}
    {{ form(component) }}
  {% elseif component.acf_fc_layout == 'heading_and_image' %}
    {{ headingAndImage(component) }}
  {% elseif component.acf_fc_layout == 'image' %}
    {{ image(component) }}
  {% elseif component.acf_fc_layout == 'image_grid' %}
    {{ imageGrid(component) }}
  {% elseif component.acf_fc_layout == 'video_content' %}
    {{ videoContent(component) }}
  {% elseif component.acf_fc_layout == 'image_and_image_grid' %}
    {{ imageAndImageGrid(component) }}
  {% elseif component.acf_fc_layout == 'callout_aside' %}
    {{ calloutAside(component) }}
  {% elseif component.acf_fc_layout == 'banner_callout' %}
    {{ bannerCallout(component) }}
  {% elseif component.acf_fc_layout == 'full_width_tout' %}
    {{ fullWidthTout(component) }}
  {% elseif component.acf_fc_layout == 'two_column' %}
    {{ twoColumn(component) }}
  {% elseif component.acf_fc_layout == 'slideshow_two_column' %}
    {{ slideshowTwoCol(component) }}
  {% elseif component.acf_fc_layout == 'image_and_text' %}
    {{ imageAndText(component) }}
  {% elseif component.acf_fc_layout == 'product_feature' %}
    {{ productFeature(component) }}
  {% elseif component.acf_fc_layout == 'steps' %}
    {{ steps(component) }}
  {% elseif component.acf_fc_layout == 'quote' %}
    {{ quote(component) }}
  {% elseif component.acf_fc_layout == 'rich_text' %}
    {{ richText(component) }}
  {% elseif component.acf_fc_layout == 'slideshow' %}
    {{ slideshow(component) }}
  {% elseif component.acf_fc_layout == 'slideshow_grid' %}
    {{ slideshowGrid(component) }}
  {% elseif component.acf_fc_layout == 'callout' %}
    {{ callout(component) }}
  {% elseif component.acf_fc_layout == 'people' %}
    {{ people(component) }}
  {% elseif component.acf_fc_layout == 'feature' %}
   {{ feature(component) }}
  {% elseif component.acf_fc_layout == 'podcast' %}
    {{ podcast(component) }}
  {% elseif component.acf_fc_layout == 'image_and_touts' %}
    {{ imageAndTouts(component) }}
  {% elseif component.acf_fc_layout == 'multiple_touts' %}
    {{ multipleTouts(component) }}
