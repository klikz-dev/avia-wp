    {{ multipleCallouts(component) }}
  {% elseif component.acf_fc_layout == 'lede' %}
    {{ lede(component) }}
  {% elseif component.acf_fc_layout == 'video' %}
    {{ video(component) }}
  {% elseif component.acf_fc_layout == 'featured_columns' %}
    {{ featuredColumns(component) }}
  {% elseif component.acf_fc_layout == 'image_and_cta' %}
    {{ imageCTA(component) }}
  {% elseif component.acf_fc_layout == 'featured_posts' %}
    {{ featuredPosts(component) }}
  {% elseif component.acf_fc_layout == 'bullets' %}
   {{ bullets(component) }}
  {% endif %}
{% endfor %}